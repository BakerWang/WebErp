package com.artwell.erp.tna.entity;

public class StyleInfo {
	private String clientName;
	private String seasonName;
	private String factoryStyleNumber;
	//clientName+seasonName+factoryStyleNumber (唯一)
	private String clientStyleNumber;
	
	public String getIdentifer() {
		return getClientName()+ "+" +getSeasonName()+ "+" +getFactoryStyleNumber();
	}
	public String getFactoryStyleNumber() {
		return factoryStyleNumber;
	}
	public void setFactoryStyleNumber(String factoryStyleNumber) {
		this.factoryStyleNumber = factoryStyleNumber;
	}
	public String getClientStyleNumber() {
		return clientStyleNumber;
	}
	public void setClientStyleNumber(String clientStyleNumber) {
		this.clientStyleNumber = clientStyleNumber;
	}
	public String getClientName() {
		return clientName;
	}
	public void setClientName(String clientName) {
		this.clientName = clientName;
	}
	public String getSeasonName() {
		return seasonName;
	}
	public void setSeasonName(String seasonName) {
		this.seasonName = seasonName;
	}
}
