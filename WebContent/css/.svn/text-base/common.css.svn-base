.messageBox {
	position: fixed;
	bottom:-200px;
	right: 0px;
	width: 300px;
	height: 40px;
	background-color: rgb(57, 78, 88);
	box-shadow:  -3px 0px 3px rgb(224, 230, 233),inset 0px -3px 3px rgb(224, 230, 233), -3px -3px 3px rgb(224, 230, 233);
	border-radius:10px 10px 0 0;
	//-webkit-animation:showMsg 1s ease-out;
	transition: all 1s;
	font-size: 20px;
	font-weight: bold;
}
.messageBox ul {
	list-style: none;
	padding-left: 0px;
	margin-top: 3px;
}
.messageBox ul li {
	color: #E1F129;
}
@-webkit-keyframes  showMsg{
0%   {bottom:-200px;}
50%  {bottom:0px;}
100% {bottom:-200px;}
}
