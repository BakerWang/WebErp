#searchDiv {
	background-color: rgb(53, 51, 51);
	color: white;
	padding-top: 15px;
	padding-bottom: 15px;
	padding-left: 10px;
	width: 919px;
}

#searchDiv input {
	width: 130px;
	height: 15px;
}

#searchDiv td {
	padding: 0px;
	padding-right: 10px;
}

#detailDiv td {
	padding: 0px;
	padding-right: 10px;
	width: 105px;
}

#cartListDiv {
	clear: both;
	position: relative;
	top: 20px;
}
#cartListDiv .title{
background-color: rgb(77, 80, 80);
color: rgb(255, 247, 50);
font-weight: bold;
}
#cartListDiv td {
	padding: 0px;
	padding-right: 10px;
	width: 105px;
}

.buttonBar {
	width: 929px;
	padding-right: 0px;
}

.buttonBar input {
	float: right;
}

#cartListDiv .buttonBar input {
	float: left;
}

.submit {
	width: 60px !important;
	height: 20px !important;
}
.button{
background-color: rgb(77, 80, 80);
color: rgb(255, 247, 50);
height: 25px;
border: none;
border-radius: 10px;
font-weight: bold;
cursor: pointer;
}
